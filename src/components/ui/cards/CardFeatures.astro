---
import Card from './CardBasic.astro'

const { title = '', subtitle, image, link, type } = Astro.props
---

<Card
	title={title}
	subtitle={subtitle}
	titleSize="xl"
	link={link}
	image={image}
	type="image-bottom"
	classes="!bg-neutral-50 !shadow-none dark:!bg-neutral-900 h-full justify-between border border-neutral-100 dark:border-neutral-800"
/>
